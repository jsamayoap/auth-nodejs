    "@babel/preset-typescript": "^7.16.0",
    "@tsed/cli-plugin-eslint": "3.15.0",
    "@tsed/cli-plugin-mocha": "3.15.0",
    "@tsed/cli-plugin-oidc-provider": "3.15.0",
    "@tsed/cli-plugin-typeorm": "3.15.0",
