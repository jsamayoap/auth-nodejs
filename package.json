    "@types/koa__router": "^8.0.9",
    "@types/koa-bodyparser": "^4.3.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-json": "^2.0.20",
    "@types/koa-qs": "^2.0.0",
